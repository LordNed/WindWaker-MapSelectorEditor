<Window x:Class="WPF_Editor.GenericModalWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GenericModalWindow" Height="163" Width="300" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid>
        <Button x:Name="m_cancelButton" Content="Button" HorizontalAlignment="Left" Margin="10,102,0,0" VerticalAlignment="Top" Width="75" Click="OnCancelButtonClick"/>
        <Button x:Name="m_confirmButton" Content="Button" HorizontalAlignment="Left" Margin="207,102,0,0" VerticalAlignment="Top" Width="75" Click="OnConfirmButtonClick"/>
        <TextBlock x:Name="m_text" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="87" Width="272"/>

    </Grid>
</Window>
